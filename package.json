{
  "name": "alarmwhitenoiseapp",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:reliability": "jest --testMatch='**/__tests__/**/*reliability*.test.(ts|tsx)' --verbose --detectOpenHandles",
    "test:alarm": "jest --testMatch='**/__tests__/**/*alarm*.test.(ts|tsx)' --verbose",
    "test:audio": "jest --testMatch='**/__tests__/**/*audio*.test.(ts|tsx)' --verbose",
    "test:integration": "jest --testMatch='**/__tests__/**/*integration*.test.(ts|tsx)' --verbose --runInBand",
    "test:performance": "jest --testMatch='**/__tests__/**/*performance*.test.(ts|tsx)' --verbose --runInBand",
    "test:ci": "jest --ci --coverage --watchAll=false --detectOpenHandles",
    "test:e2e:ios": "detox test --configuration ios.sim.release",
    "test:e2e:android": "detox test --configuration android.emu.release",
    "build:detox:ios": "detox build --configuration ios.sim.release",
    "build:detox:android": "detox build --configuration android.emu.release",
    "test:report": "node scripts/test-reporter.js",
    "test:dashboard": "npm run test:ci && npm run test:report",
    "test:all": "npm run test:ci && npm run test:e2e:ios && npm run test:e2e:android && npm run test:report"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.5",
    "@react-native-community/slider": "^4.5.6",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/drawer": "^7.5.8",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@supabase/supabase-js": "^2.57.4",
    "expo": "~53.0.22",
    "expo-apple-authentication": "^7.2.4",
    "expo-auth-session": "^6.2.1",
    "expo-av": "^15.1.7",
    "expo-background-fetch": "^13.1.6",
    "expo-blur": "^14.1.5",
    "expo-crypto": "^14.1.5",
    "expo-haptics": "^14.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "^7.1.7",
    "expo-local-authentication": "^16.0.5",
    "expo-notifications": "^0.31.4",
    "expo-secure-store": "~14.2.4",
    "expo-status-bar": "~2.2.3",
    "expo-task-manager": "^13.1.6",
    "immer": "^10.1.3",
    "react": "19.1.1",
    "react-native": "^0.81.4",
    "react-native-crypto-js": "^1.0.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-purchases": "^9.3.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-uuid": "^2.0.3",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/cli": "^0.26.8",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/metro-config": "^0.81.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.13",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-root-import": "^6.6.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "babel-plugin-transform-remove-undefined": "^0.5.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "detox": "^20.41.1",
    "eslint": "^8.57.1",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "~29.7.0",
    "jest-html-reporters": "^3.1.7",
    "prettier": "^3.6.2",
    "react-native-reanimated": "~3.17.4",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-worklets": "^0.5.1",
    "react-test-renderer": "^19.1.1",
    "ts-jest": "^29.4.1",
    "typescript": "~5.9.2"
  },
  "private": true
}
